{
  "compilerOptions": {
    "target":"ES2020",
    "lib": [
      "es2020",
      "dom"
    ],
    "incremental": false,
    "module": "commonjs",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "strict": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "isolatedModules": true,
    "skipLibCheck": true
  },
  "include": [
    "src/*",
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}